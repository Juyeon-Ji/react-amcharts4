{
  "name": "react-amcharts4",
  "version": "0.0.1",
  "main": "./lib/index.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "author": {
    "name": "Abhishek Ramesh",
    "email": "abhiisheek@hotmail.com"
  },
  "keywords": [
    "amcharts4",
    "react"
  ],
  "files": [
    "lib"
  ],
  "private": true,
  "peerDependencies": {
    "@amcharts/amcharts4": "^4.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.5.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "autoprefixer": "7.1.2",
    "babel-jest": "^24.8.0",
    "babel-jest-amcharts": "0.0.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-app": "^9.0.0",
    "babel-runtime": "6.26.0",
    "cross-env": "^5.2.0",
    "dotenv": "4.0.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "fs-extra": "3.0.1",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.1",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "raf": "^3.4.0",
    "rimraf": "^2.6.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "build:commonjs": "cross-env NODE_ENV=production babel ./src --out-dir ./lib --ignore src/**/*.test.js",
    "build:copy-files": "node scripts/copyFilesForPubBuild.js",
    "prebuild": "rimraf lib",
    "build": "npm-run-all --parallel build:commonjs build:copy-files",
    "test": "jest --coverage -u",
    "test:watch": "jest --watch --coverage",
    "prepublish": "npm run build",
    "publish": "cd ./lib && npm publish"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/node_modules/raf/polyfill.js",
      "<rootDir>/scripts/enzymeSetup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest-amcharts",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!(@amcharts)\\/).+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coverageReporters": [
      "cobertura",
      "text",
      "html"
    ]
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-modules-commonjs"
    ],
    "presets": [
      "react-app"
    ],
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    }
  }
}
